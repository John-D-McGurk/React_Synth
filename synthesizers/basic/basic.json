{
  "panels": {
    "pitchMod": true,
    "controls": {
      "columns": 1,
      "inputs": {
        "gain": { "type": "Knob" },
        "on": { "type": "Button" }
      }
    },
    "filter": {
      "inputs": {
        "on": {"type": "Checkbox"},
        "frequency": { "type": "Knob", "max": 22000 },
        "Q": { "type": "Knob", "max": 1, "step": 0.01 },
        "type": {
          "type": "Dropdown",
          "contents": [
            "lowpass",
            "highpass",
            "bandpass",
            "lowshelf",
            "highshelf",
            "notch"
          ]
        }
      }
    },
    "envelope": true,
    "octave": true
  },

  "inputs": {
    "keyboard": true,
    "octaves": [3, 4]
  }
}

{
  "panels": {
    "pitchMod": true,
    "controls": {
      "columns": 1,
      "inputs": {
        "gain": { "type": "Knob" }
      }
    },
    "filter": {
      "inputs": {
        "on": { "type": "Checkbox" },
        "frequency": { "type": "Knob", "max": 10000 },
        "Q": { "type": "Knob", "min": 0.0001, "max": 50 },
        "gain": { "type": "Knob", "min": -40, "max": 40, "step": 0.01 },
        "wet": { "type": "Knob" },
        "type": {
          "type": "Dropdown",
          "contents": [
            "lowpass",
            "highpass",
            "bandpass",
            "lowshelf",
            "highshelf",
            "notch"
          ]
        }
      }
    },
    "envelope": true,
    "octave": true
  },

  "inputs": {
    "keyboard": true
  }
}
